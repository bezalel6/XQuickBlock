    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@types/styled-components": "^5.1.34",
    "framer-motion": "^12.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.17"
  },
  "devDependencies": {
    "@types/chrome": "0.0.158",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
