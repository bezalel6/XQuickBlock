    "archiver": "^6.0.2",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "glob": "^7.1.6",
    "jest": "^29.5.0",
    "lit-html": "^3.3.0",
    "prettier": "^3.5.3",
    "rimraf": "^3.0.2 ",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.0.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "^5.0.0"
  }
